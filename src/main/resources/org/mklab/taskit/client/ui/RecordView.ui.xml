<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:ce="urn:import:com.google.gwt.user.cellview.client"
	xmlns:mk="urn:import:org.mklab.taskit.client.ui">

	<g:VerticalPanel>
		<g:HorizontalPanel>
			<g:Label ui:field='rankLabel'>Rank:</g:Label>
			<g:Label ui:field='rank'></g:Label>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label ui:field='achievementRatioLabel'>Score:</g:Label>
			<g:Label ui:field='achievementRatio'></g:Label>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label ui:field='deviationLabel'>Deviation:</g:Label>
			<g:Label ui:field='deviation'></g:Label>
		</g:HorizontalPanel>
		<g:HTML>
			<hr />
		</g:HTML>
		<g:HorizontalPanel>
			<g:Label ui:field='averageLabel'>Average:</g:Label>
			<g:Label ui:field='average'></g:Label>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label ui:field='standardDeviationLabel'>Standard Deviation:</g:Label>
			<g:Label ui:field='standardDeviation'></g:Label>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 