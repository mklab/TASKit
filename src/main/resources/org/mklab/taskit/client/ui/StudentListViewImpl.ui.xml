<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:ce="urn:import:com.google.gwt.user.cellview.client"
	xmlns:mk="urn:import:org.mklab.taskit.client.ui">

	<g:DockLayoutPanel unit='EM'>
		<g:west size='10'>
			<g:ScrollPanel>
				<ce:CellList ui:field='list'></ce:CellList>
			</g:ScrollPanel>
		</g:west>
		<g:center>
			<g:ScrollPanel>
				<g:VerticalPanel>
					<g:HorizontalPanel>
						<g:Label>User ID:</g:Label>
						<g:Label ui:field='userId'></g:Label>
					</g:HorizontalPanel>
					<g:HorizontalPanel>
						<g:Label>User Name:</g:Label>
						<g:Label ui:field='userName'></g:Label>
					</g:HorizontalPanel>
					<mk:StudentScorePanel ui:field='panel'></mk:StudentScorePanel>
					<g:Button ui:field='uncallButton'>Uncall</g:Button>
				</g:VerticalPanel>
			</g:ScrollPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 