<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:ce="urn:import:com.google.gwt.user.cellview.client"
	xmlns:mk="urn:import:org.mklab.taskit.client.ui">

	<ui:style>
		.message {
			background-color : gray;
		}
	</ui:style>
	<g:ScrollPanel>
		<g:VerticalPanel>
			<g:Label ui:field='messageLabel' styleName='{style.message}'></g:Label>
			<ce:CellList ui:field='list'></ce:CellList>
		</g:VerticalPanel>
	</g:ScrollPanel>

</ui:UiBinder> 