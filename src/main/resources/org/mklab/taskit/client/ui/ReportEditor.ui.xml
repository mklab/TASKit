<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:ce="urn:import:com.google.gwt.user.cellview.client"
	xmlns:d="urn:import:com.google.gwt.user.datepicker.client">

	<g:VerticalPanel>
		<g:Grid>
			<g:row>
				<g:cell>Title:</g:cell>
				<g:customCell>
					<g:TextBox ui:field='title'></g:TextBox>
				</g:customCell>
			</g:row>
			<g:row>
				<g:cell>Description:</g:cell>
				<g:customCell>
					<g:TextArea ui:field='description'></g:TextArea>
				</g:customCell>
			</g:row>
			<g:row>
				<g:cell>Period:</g:cell>
				<g:customCell>
					<d:DateBox ui:field='period'></d:DateBox>
				</g:customCell>
			</g:row>
			<g:row>
				<g:cell>Point:</g:cell>
				<g:customCell>
					<g:TextBox ui:field='point'></g:TextBox>
				</g:customCell>
			</g:row>
			<g:row>
				<g:cell>Lecture:</g:cell>
				<g:customCell>
					<g:ValueListBox ui:field='lecture'></g:ValueListBox>
				</g:customCell>
			</g:row>
		</g:Grid>
		<g:Button ui:field='changeButton'>Change</g:Button>
	</g:VerticalPanel>

</ui:UiBinder> 