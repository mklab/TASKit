<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:ce="urn:import:com.google.gwt.user.cellview.client"
	xmlns:mk="urn:import:org.mklab.taskit.client.ui.cw">

	<ui:style>
		.list {
		border:solid 1px gray;
		}
	</ui:style>

	<g:DockLayoutPanel unit='EM'>
		<g:west size='10'>
			<g:ScrollPanel styleName='{style.list}'>
				<ce:CellList ui:field='list'></ce:CellList>
			</g:ScrollPanel>
		</g:west>
		<g:center>
			<mk:StudentScorePanel ui:field='panel'></mk:StudentScorePanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 